{
    "name": "xaraya/images",
    "type": "xaraya-module",
    "description": "Handles image manipulation with resizing/cropping/scaling/rotating and various filters",
    "homepage": "https://github.com/xaraya-modules/images",
    "license": "GPL-2.0-or-later",
    "require": {},
    "suggest": {
        "james-heinrich/phpthumb": "The PHP thumbnail generator"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/JamesHeinrich/phpThumb"
        }
    ],
    "autoload": {
        "classmap": ["./"],
        "exclude-from-classmap": ["/vendor/"]
    }
}
